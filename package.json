{
  "name": "react-native-ssl-pinning",
  "version": "1.5.7",
  "description": "React-Native Ssl pinning using OkHttp 3 in Android, and AFNetworking on iOS.",
  "main": "./lib/commonjs/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "bob build"
  },
  "keywords": [
    "react-native"
  ],
  "types": "./index.d.ts",
  "author": "Max Toyberman",
  "dependencies": {
    "q": "^1.4.1"
  },
  "license": "MIT",
  "homepage": "https://github.com/MaxToyberman/react-native-ssl-pinning#README",
  "peerDependencies": {
    "react-native": "0.75.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaxToyberman/react-native-ssl-pinning.git"
  },
  "codegenConfig": {
    "name": "reactnativesslpinning",
    "type": "modules",
    "jsSrcsDir": "./src/specs",
    "outputDir": {
      "ios": "ios/generated",
      "android": "android/generated"
    },
    "android": {
      "javaPackageName": "com.toyberman"
    }
  },
  "packageManager": "yarn@3.6.4",
  "devDependencies": {
    "react-native-builder-bob": "^0.30.2"
  },
  "source": "./index.js",
  "module": "./lib/module/index.js",
  "exports": {
    ".": {
      "import": {
        "default": "./lib/module/index.js"
      },
      "require": {
        "default": "./lib/commonjs/index.js"
      }
    }
  },
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ]
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}
